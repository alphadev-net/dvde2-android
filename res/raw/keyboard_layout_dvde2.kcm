# German Dvorak Type2 keyboard layout
# UTF-8 encoded
#
type OVERLAY
#Home row
key A {
	label:	'A'
	base:	'a'
	capslock,shift:	'A'
	alt:	'\u0040'
}
key S {
	label:	'O'
	base:	'o'
	capslock,shift:	'O'
}
key D {
	label:	'E'
	base:	'e'
	capslock,shift: 	'E'
}
key F {
	label:	'I'
	base:	'i'
	capslock,shift:	'I'
}
key G {
	label:	'U'
	base:	'u'
	capslock,shift: 	'U'
}
key H {
	label:	'H'
	base:	'h'
	capslock,shift: 	'H'
}
key J {
	label:	'D'
	base:	'd'
	capslock,shift: 	'D'
}
key K {
	label:	'R'
	base:	'r'
	capslock,shift: 	'R'
}
key L {
	label:	'N'
	base:	'n'
	capslock,shift: 	'N'
}
key SEMICOLON {
	label: 'S'
	base: 's'
	capslock,shift: 'S'
}
key APOSTROPHE {
	label: 'L'
	base: 'l'
	capslock,shift: 'L'
}
key BACKSLASH {
	label: '-'
	base: '-'
	shift: '_'
}

#Top row
key Q {
	label: '\u00DC'
	base: '\u00FC'
	capslock,shift: '\u00DC'
}
key W {
	label: ','
	base: ','
	shift: ';'
}
key E {
	label: '.'
	base: '.'
	shift: ':'
}
key R {
	label: 'P'
	base: 'p'
	capslock,shift: 'P'
}
key T {
	label: 'Y'
	base: 'y'
	capslock,shift: 'Y'
}
key Y {
	label: 'F'
	base: 'f'
	capslock,shift: 'F'
}
key U {
	label: 'G'
	base: 'g'
	capslock,shift: 'G'
}
key I {
	label: 'C'
	base: 'c'
	capslock,shift: 'C'
}
key O {
	label: 'T'
	base: 't'
	capslock,shift: 'T'
}
key P {
	label: 'Z'
	base: 'z'
	capslock,shift: 'Z'
}
key LEFT_BRACKET {
	label: '?'
	base: '?'
	shift: '\u00DF'
}
key RIGHT_BRACKET {
	label: '/'
	base: '/'
	shift: '\\'
}
# Bottom row
key Z {
	label: '\u00D6'
	base: '\u00F6'
	capslock,shift: '\u00D6'
}
key X {
	label: 'Q'
	base: 'q'
	capslock,shift: 'Q'
	alt: '\u0040'
}
key C {
	label: 'J'
	base: 'j'
	capslock,shift: 'J'
}
key V {
	label: 'K'
	base: 'k'
	capslock,shift: 'K'
}
key B {
	label: 'X'
	base: 'x'
	capslock,shift: 'X'
}
key N {
	label: 'B'
	base: 'b'
	capslock,shift: 'B'
}
key M {
	label: 'M'
	base: 'm'
	capslock,shift: 'M'
}
key COMMA {
	label: 'W'
	base: 'w'
	capslock,shift: 'W'
}
key PERIOD {
	label: 'V'
	base: 'v'
	capslock,shift: 'V'
}
key SLASH {
	label: '#'
	base: '#'
	shift: '\''
}

#Special Keys
key GRAVE {
	label: '\u00C4'
	base: '\u00E4'
	capslock,shift: '\u00C4'
}
